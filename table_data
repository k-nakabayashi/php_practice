CREATE TABLE `free_auction`.`items` ( `item_id` INT(11) NOT NULL , `item_name` VARCHAR(256) NOT NULL , `item_img1` VARCHAR(256) NOT NULL , `item_img2` VARCHAR(256) NULL , `item_img3` VARCHAR(256) NULL , `item_title` VARCHAR(256) NOT NULL , `item_thumb` VARCHAR(256) NOT NULL , `item_detail` VARCHAR(256) NOT NULL , `item_category` VARCHAR(256) NOT NULL , `item_status` VARCHAR(256) NOT NULL , `item_area` VARCHAR(256) NOT NULL , `item_good_count` INT(11) NOT NULL DEFAULT '0' , `ehibitor_id` INT(11) NOT NULL , `chat_board_count` INT(11) NULL DEFAULT '0' , `at_created` TIMESTAMP NULL DEFAULT CURRENT_TIMESTAMP , `at_updated` TIMESTAMP on update CURRENT_TIMESTAMP NULL DEFAULT CURRENT_TIMESTAMP , `delete_flg` BOOLEAN NULL DEFAULT FALSE , PRIMARY KEY (`item_id`)) ENGINE = InnoDB;